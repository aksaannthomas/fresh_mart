        app:layout_constraintTop_toBottomOf="@+id/cinnamon" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="79dp"
        android:layout_height="31dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:text="Rs. 35"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/turmeric" />

    <ImageView
        android:id="@+id/pepper"
        android:layout_width="157dp"
        android:layout_height="120dp"
        android:layout_marginTop="68dp"
        app:layout_constraintEnd_toStartOf="@+id/fenugreek"
        app:layout_constraintHorizontal_bias="0.614"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        app:srcCompat="@drawable/pepper" />

    <ImageView
        android:id="@+id/fenugreek"
        android:layout_width="154dp"
        android:layout_height="144dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:srcCompat="@drawable/fenugreek" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rs. 680"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView34"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pepper"
        app:layout_constraintVertical_bias="0.067" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="72dp"
        android:layout_height="21dp"
        android:layout_marginEnd="80dp"
        android:layout_marginRight="80dp"
        android:layout_marginBottom="120dp"
        android:text="Rs. 105"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fenugreek"
        app:layout_constraintVertical_bias="0.096" />

</androidx.constraintlayout.widget.ConstraintLayout>